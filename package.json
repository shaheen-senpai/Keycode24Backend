{
  "name": "ums",
  "private": true,
  "version": "0.0.1",
  "description": "Description",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "TZ=UTC nest start",
    "start:debug": "nest start --debug --watch",
    "start:server": "node dist/main",
    "start:server-with-cron": "node dist/main --additionalModules=cron",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/common/config/ormconfig.ts",
    "typeorm-server": "node ./node_modules/typeorm/cli.js -d ./dist/common/config/ormconfig.js",
    "run-migrations": "npm run typeorm -- migration:run",
    "run-server-migrations": "npm run typeorm-server -- migration:run",
    "generate-migration": "npm run typeorm -- migration:generate ./src/common/migrations/$npm_config_greeting",
    "create-migration:win": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/common/migrations/%npm_config_name%",
    "generate-migration:win": "npm run typeorm -- migration:generate ./src/common/migrations/%npm_config_name%",
    "create-migration": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/common/migrations/$npm_config_name",
    "rollback-migration": "npm run typeorm -- migration:revert",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/cache-manager": "^2.2.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.1",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/terminus": "^10.2.1",
    "@nestjs/typeorm": "^10.0.1",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.4.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "handlebars": "^4.7.8",
    "joi": "^17.12.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.11.3",
    "source-map-support": "^0.5.21",
    "typeorm": "0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^10.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "ts-morph": "^20.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1"
  }
}
